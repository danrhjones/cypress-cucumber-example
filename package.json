{
  "name": "cypress_framework_skeleton",
  "version": "1.0.0",
  "description": "An example for Cypress and Cucumber usage",
  "main": "index.js",
  "scripts": {
    "report": "node index.js",
    "test": "cypress run --spec '**/*.feature'; npm run report"
  },
  "keywords": [],
  "author": "",
  "license": "ISC",
  "dependencies": {
    "cucumber-html-reporter": "^5.0.2",
    "cypress": "^3.7.0",
    "cypress-cucumber-preprocessor": "1.18.0",
    "faker": "^4.1.0"
  },
  "devDependencies": {
    "eslint": "^6.8.0",
    "eslint-config-airbnb-base": "^14.0.0",
    "eslint-config-prettier": "^6.9.0",
    "eslint-plugin-import": "^2.20.0",
    "eslint-plugin-prettier": "^3.1.2",
    "husky": "^4.0.10",
    "lint-staged": "^10.0.1",
    "prettier": "1.19.1"
  },
  "cypress-cucumber-preprocessor": {
    "nonGlobalStepDefinitions": true,
    "cucumberJson": {
      "generate": true,
      "outputFolder": "cucumber-json",
      "filePrefix": "cucumber-"
    }
  },
  "lint-staged": {
    "*.js": [
      "eslint",
      "eslint --fix",
      "git add"
    ]
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged"
    }
  }
}
